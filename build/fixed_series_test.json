{
  "simulation_config": {
    "num_trials": 1
  },
  "inputs": {
    "initial_value": {
      "type": "fixed",
      "value": 100.0
    },
    "specific_growth_rates": {
      "type": "fixed",
      "value": [0.05, 0.1, 0.02]
    }
  },
  "operations": [
    {
      "op_code": "compound_series",
      "args": ["initial_value", "specific_growth_rates"],
      "result": "final_compounded_series"
    },
    {
      "op_code": "sum_series",
      "args": ["final_compounded_series"],
      "result": "sum_of_compounded_series"
    }
  ],
  "output_variable": "sum_of_compounded_series"
}
