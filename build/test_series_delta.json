{
  "simulation_config": {
    "num_trials": 1000
  },
  "inputs": {
    "base_value": {
      "type": "fixed",
      "value": 100.0
    },
    "growth_rate": {
      "type": "fixed",
      "value": 0.1
    },
    "forecast_years": {
      "type": "fixed",
      "value": 5
    }
  },
  "operations": [
    {
      "op_code": "grow_series",
      "args": ["base_value", "growth_rate", "forecast_years"],
      "result": "my_series"
    },
    {
      "op_code": "series_delta",
      "args": ["my_series"],
      "result": "delta_of_my_series"
    },
    {
      "op_code": "get_element",
      "args": ["delta_of_my_series", -1],
      "result": "final_delta_value"
    }
  ],
  "output_variable": "final_delta_value"
}
