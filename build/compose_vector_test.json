{
  "simulation_config": {
    "num_trials": 10000
  },
  "inputs": {
    "r1_dist": {
      "type": "distribution",
      "dist_name": "Normal",
      "params": { "mean": 10.0, "stddev": 1.0 }
    },
    "r2_dist": {
      "type": "distribution",
      "dist_name": "PERT",
      "params": { "min": 20.0, "mostLikely": 25.0, "max": 30.0 }
    },
    "r3_dist": {
      "type": "distribution",
      "dist_name": "Uniform",
      "params": { "min": 5.0, "max": 15.0 }
    },
    "fixed_scalar": {
      "type": "fixed",
      "value": 100.0
    }
  },
  "operations": [
    {
      "op_code": "compose_vector",
      "args": ["r1_dist", "r2_dist", "r3_dist", "fixed_scalar"],
      "result": "composed_output_vector"
    },
    {
      "op_code": "sum_series",
      "args": ["composed_output_vector"],
      "result": "sum_of_vector_elements"
    }
  ],
  "output_variable": "sum_of_vector_elements"
}
